/*******************************
         Theme Overrides
*******************************/
.dj-hamburger { 
    position: absolute;
    position: sticky;
    top: 0.2rem;
    
    z-index: 1;

    .button {
        position: absolute;
        right: 0;
    }

}

@media only screen and (min-width: 992px) {
    .dj-hamburger,
    .ui.visible.left.overlay.sidebar { 
        display: none;
    }
}